<?xml version="1.0"?>
<launch>
    <node   pkg="tello_controller"  type="show_way_point" name="way_point" output="screen">
        <param name="prefix"            type="string"   value="way_point" />
        <param name="wpFrame"           type="string"   value="camera7_color_optical_frame"/>
        <param name="way_point_file"    type="string"   value="$(find tello_controller)/way_point/20200518/path_minitype.txt" />
        <param name="all_pub"           type="bool"     value="true"/>
    </node>
    <node   pkg="tello_controller"  type="show_way_point" name="sample" output="screen">
        <param name="prefix"            type="string"   value="sample" />
        <param name="wpFrame"           type="string"   value="camera7_color_optical_frame"/>
        <param name="way_point_file"    type="string"   value="$(find tello_controller)/way_point/20200518/mini_sample_pnt.txt" />
        <param name="all_pub"           type="bool"     value="true"/>
    </node>
</launch>
